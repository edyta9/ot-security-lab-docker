    {
        "id": "61fe787397be250c",
        "type": "function",
        "z": "c9f33fdb20a4d5e1",
        "name": "alarm logic",
        "func": "// PLC1 – feeder / transformer high alarm logic\n\nvar v = Number(msg.payload);          // process value z HR0\nvar threshold = flow.get('alarm_threshold') || 60;\nvar elevatedMin = threshold - 10;\n\n// poprzedni stan (domyślnie OK)\nvar prevLevel = flow.get('plc1_prev_level') || \"OK\";\nvar currentLevel;\n\nvar uiMsg = {};\nvar logMsg = null;\nvar status = {};\n\n// wyznacz aktualny poziom alarmu\nif (v >= threshold) {\n    currentLevel = \"ALARM_HIGH\";\n    uiMsg.payload = \"ALARM HIGH: \" + v + \" (threshold = \" + threshold + \")\";\n    status.fill = \"red\";\n} else if (v >= elevatedMin) {\n    currentLevel = \"WARNING_HIGH\";\n    uiMsg.payload = \"WARNING HIGH: \" + v + \" (threshold = \" + threshold + \")\";\n    status.fill = \"orange\";\n} else {\n    currentLevel = \"OK\";\n    uiMsg.payload = \"OK: \" + v + \" (threshold = \" + threshold + \")\";\n    status.fill = \"green\";\n}\n\n// zapamiętaj aktualny poziom\nflow.set('plc1_prev_level', currentLevel);\n\n// logujemy TYLKO, jeśli stan się zmienił\nif (currentLevel !== prevLevel) {\n    logMsg = {\n        payload: {\n            plc: \"PLC1\",\n            subsystem: \"FEEDER\",\n            level: currentLevel,\n            value: v,\n            threshold: threshold,\n            timestamp: new Date().toISOString()\n        }\n    };\n}\n\n// UI idzie górnym wyjściem, log dolnym\nuiMsg.ui_control = status;\nreturn [uiMsg, logMsg];",
        "outputs": 2,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 610,
        "y": 320,
        "wires": [
            [
                "84a273ee87f2c29c"
            ],
            [
                "547de16dd9f27206"
            ]
        ]
